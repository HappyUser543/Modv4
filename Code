game.Workspace.CurrentRooms.ChildAdded:connect(function(room)
    task.wait(1)
    if room:FindFirstChild('PathfindNodes') then
        local nodes = room.PathfindNodes:Clone()
        nodes.Parent = room
        nodes.Name = 'nodes'
    end
    if room == 50 then
        task.wait(5)
        local NewFigureAmbient = getGitSoundId(
            'https://github.com/Sosnen/Ping-s-Dumbass-projects-/raw/main/Figure%20Ambience.mp3?raw=true',
            'Figure_Ambient_Hardcore_V4'
        )
        NewFigureAmbient.Parent = room
        NewFigureAmbient.Looped = true
        NewFigureAmbient:Play()
        local nodes = room.FigureSetup.FigureNodes:Clone()
        Nodes.Parent = room
        Nodes.Name = 'nodes'
    end
end)

