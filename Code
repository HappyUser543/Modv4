game:GetService('ReplicatedStorage').GameData.LatestRoom.Changed
    :Connect(function()
        local room =
            workspace.CurrentRooms[game:GetService('ReplicatedStorage').GameData.LatestRoom.Value]
        if room:FindFirstChild('Assets') then
            local paintings = {}
            for i = 1, #room:FindFirstChild('Assets'):GetChildren(), 1 do
                local painting = room:FindFirstChild('Assets'):GetChildren()[i]
                if painting.Name:find('Painting') then
                    table.insert(paintings, painting)
                end
            end
            if #paintings > 0 then
                local currentpainting
                if paintings[#paintings] then
                    painters = painters + 1
                    currentpainting = paintings[#paintings]
                else
                    currentpainting = paintings[1]
                end
                if currentpainting then
                    if currentpainting:FindFirstChild('Canvas') then
                        local selected = paints[math.random(1, #paints)]
                        if currentpainting:FindFirstChild('InteractPrompt') then
                            local cloning = currentpainting
                                :FindFirstChild('InteractPrompt')
                                :Clone()
                            cloning.Name = 'fakeInteract'
                            cloning.Parent = currentpainting
                            currentpainting
                                :FindFirstChild('InteractPrompt')
                                :Destroy()
                            local t = cloning.Triggered:Connect(function()
                                require(
                                    game.Players.LocalPlayer.PlayerGui.MainUI.Initiator.Main_Game
                                ).caption(
                                    string.gsub(
                                        'This painting is titled "NAMEOFTHING"',
                                        'NAMEOFTHING',
                                        selected[2]
                                    ),
                                    true
                                )
                            end)
                        end
                        currentpainting
                            :FindFirstChild('Canvas')
                            :FindFirstChildOfClass('SurfaceGui')
                            :FindFirstChildOfClass('ImageLabel').Image =
                            selected[1]
                    end
                end
            end
        end
    end)

for i = 1, #game.ReplicatedStorage.Misc.Eyes:GetDescendants(), 1 do
    local eye = game.ReplicatedStorage.Misc.Eyes:GetDescendants()[i]
    if eye.Name == 'Eye' and eye.Parent.Name ~= 'Eye' then
        eye.Veins.Decal.Texture = 'rbxassetid://1882220622'
        eye.Veins.Decal.Color3 = Color3.fromRGB(0, 0, 0)
    end
end
