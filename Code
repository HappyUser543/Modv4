                    ['url'] = 'https://media.discordapp.net/attachments/1335139056669163583/1340319178724474941/Screenshot_2025-02-15_135312.png?ex=68ca13c2&is=68c8c242&hm=605c619c4eccfd985e2e29a98fd3cd42cf056495c178fb0f7247b228f96a4b2a&=&format=webp&quality=lossless&width=1115&height=833',
                },
                ['fields'] = {
                    {
